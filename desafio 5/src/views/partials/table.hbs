<table class="table">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">NOMBRE</th>
      <th scope="col">EDAD</th>
      <th scope="col">FOTO</th>
    </tr>
  </thead>
  <tbody>

    {{#each customers}}
    <tr>
        <th scope="row">{{this.id}}</th>
        <td>{{this.name}}</td>
        <td>{{this.age}}</td>
        <td>
          <img src={{this.picture}} alt="img" width="50">
        </td>
      </tr>  
    {{/each}}
  
  

  </tbody>

  
</table>